	data-prop-width="<%= component['width'] %>"
	data-prop-height="<%= component['height'] %>"
	src="<%= component['src'] || 'http://placehold.it/350x150' %>"
	alt="<%= component['alt'] || 'Alt text here' %>"
	width="<%= component['width'] %>"
	height="<%= component['height'] %>"
>